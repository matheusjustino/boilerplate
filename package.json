		"@babel/preset-typescript": "^7.22.15",
		"@types/node": "^20.6.3",
		"@types/react": "^18.2.22",
		"@types/react-dom": "^18.2.7",
		"@types/styled-components": "^5.1.27",
		"@typescript-eslint/eslint-plugin": "^5.62.0",
		"@typescript-eslint/parser": "^5.62.0",
